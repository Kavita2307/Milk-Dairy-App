{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },

    "preview": {
      "cache": {
        "disabled": false
      },
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },

    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "cache": {
        "disabled": false
      },
      "autoIncrement": true
    }
  },

  "submit": {
    "production": {}
  }
}
